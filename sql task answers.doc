<html xmlns:o='urn:schemas-microsoft-com:office:office' xmlns:w='urn:schemas-microsoft-com:office:word' xmlns='http://www.w3.org/TR/REC-html40'><head><meta charset='utf-8'><title>Text To Word</title></head><body><p>1.SELECT FIRST_NAME AS WORKER_NAME FROM Worker;</p>
<p>&nbsp;2.SELECT UPPER(FIRST_NAME) AS FIRST_NAME_UPPERCASE FROM Worker;</p>
<p>3.SELECT DISTINCT DEPARTMENT FROM Worker;</p>
<p>4.SELECT SUBSTRING(FIRST_NAME, 1, 3) AS FIRST_THREE_CHAR FROM Worker;&nbsp;</p>
<p>5.SELECT LOCATE('a', FIRST_NAME) AS POSITION_OF_a FROM Worker WHERE FIRST_NAME = 'Amitabh';&nbsp;</p>
<p>6.SELECT RTRIM(FIRST_NAME) AS FIRST_NAME_TRIMMED FROM Worker;</p>
<p>&nbsp;7.SELECT LTRIM(DEPARTMENT) AS DEPARTMENT_TRIMMED FROM Worker;</p>
<p>&nbsp;8.SELECT DISTINCT DEPARTMENT, LENGTH(DEPARTMENT) AS DEPARTMENT_LENGTH FROM Worker;</p>
<p>&nbsp;9.SELECT REPLACE(FIRST_NAME, 'a', 'A') AS FIRST_NAME_MODIFIED FROM Worker;</p>
<p>10.SELECT CONCAT(FIRST_NAME, ' ', LAST_NAME) AS COMPLETE_NAME FROM Worker;</p>
<p>11.SELECT * FROM Worker ORDER BY FIRST_NAME ASC;&nbsp;</p>
<p>12.SELECT * FROM Worker ORDER BY FIRST_NAME ASC, DEPARTMENT DESC; 13.SELECT * FROM Worker WHERE FIRST_NAME IN ('Vipul', 'Satish');&nbsp;</p>
<p>14.SELECT * FROM Worker WHERE DEPARTMENT = 'Admin'; 15.SELECT * FROM Worker WHERE FIRST_NAME LIKE '%a%';&nbsp;</p>
<p>16.SELECT * FROM Worker WHERE FIRST_NAME LIKE '%a';</p>
<p>&nbsp;17.SELECT * FROM Worker WHERE FIRST_NAME LIKE '______h';</p>
<p>18.SELECT * FROM Worker WHERE SALARY BETWEEN 100000 AND 500000;</p>
<p>&nbsp;19.SELECT * FROM Worker WHERE MONTH(JOINING_DATE) = 2 AND YEAR(JOINING_DATE) = 2014;</p>
<p>20.SELECT COUNT(*) AS Admin_Employee_Count FROM Worker WHERE DEPARTMENT = 'Admin';</p>
<p>&nbsp;21.SELECT FIRST_NAME, LAST_NAME FROM Worker WHERE SALARY BETWEEN 50000 AND 100000;</p>
<p>22.SELECT DEPARTMENT, COUNT(*) AS Worker_Count FROM Worker GROUP BY DEPARTMENT ORDER BY Worker_Count DESC;&nbsp;</p>
<p>23.SELECT w.* FROM Worker w JOIN Title t ON w.WORKER_ID = t.WORKER_REF_ID WHERE t.WORKER_TITLE = 'Manager';</p>
<p>&nbsp;24.SELECT Worker_Title,AFFECTED_FROM,COUNT(*) FROM Title GROUP BY WORKER_TITLE,AFFECTED_FROM HAVING COUNT(*)&gt;1;</p>
<p>&nbsp;25.WITH NumberedRows AS (SELECT *, ROW_NUMBER() OVER (ORDER BY (SELECT NULL)) AS RowNum FROM Title);</p>
<p>&nbsp;26.SELECT * FROM NumberedRows WHERE RowNum % 2 &lt;&gt; 0;</p>
<p>27.SELECT * FROM Worker WHERE MOD(WORKER_ID,2)&lt;&gt;0;&nbsp;</p>
<p>28.SELECT*FROM Worker WHERE MOD(WORKER_ID,2)=0;</p>
<p>&nbsp;29.CREATE TABLE WorkerClone AS SELECT * FROM Worker;</p>
<p>&nbsp;30.SELECT * FROM Worker INTERSECT(SELECT * FROM WorkerClone);</p>
<p>31.SELECT w.* FROM Worker w LEFT JOIN Title t ON w.WORKER_ID = t.WORKER_REF_ID WHERE t.WORKER_REF_ID IS NULL;&nbsp;</p>
<p>32.SELECT CURRENT_TIMESTAMP AS CurrentDateTime;</p>
<p>33.SELECT * FROM Worker LIMIT 10;&nbsp;</p>
<p>34.SELECT DISTINCT Salary FROM Worker ORDER BY Salary DESC LIMIT 1 OFFSET 4;</p>
<p>35.SELECT DISTINCT SALARY FROM Worker w1 WHERE 5 = (SELECT COUNT(DISTINCT SALARY) FROM Worker w2 WHERE w2.SALARY &gt;= w1.SALARY);&nbsp;</p>
<p>36.ELECT SALARY, GROUP_CONCAT(FIRST_NAME) AS EMPLOYEES_WITH_SAME_SALARY FROM Worker GROUP BY SALARY HAVING COUNT(*) &gt; 1;&nbsp;</p>
<p>37.SELECT MAX(Salary) AS SecondHighestSalary FROM Worker WHERE Salary &lt; (SELECT MAX(Salary) FROM Worker);&nbsp;</p>
<p>38.SELECT * FROM Worker UNION ALL SELECT * FROM Worker LIMIT 1;</p>
<p>39.SELECT * FROM Worker W JOIN Bonus B ON W.WORKER_ID = B.WORKER_REF_ID;</p>
<p>&nbsp;40.SELECT * FROM Worker WHERE WORKER_ID &lt;= (SELECT COUNT(*)/2 FROM Worker);</p>
<p>&nbsp;41.SELECT DEPARTMENT FROM Worker GROUP BY DEPARTMENT HAVING COUNT(*) &lt; 5;</p>
<p>42.SELECT DEPARTMENT, COUNT(*) AS NUM_WORKERS FROM Worker GROUP BY DEPARTMENT;&nbsp;</p>
<p>43.SELECT * FROM Worker ORDER BY WORKER_ID DESC LIMIT 1;&nbsp;</p>
<p>44.SELECT * FROM Worker LIMIT 1;&nbsp;</p>
<p>45.SELECT * FROM Worker ORDER BY WORKER_ID DESC LIMIT 5;</p>
<p>&nbsp;46.SELECT W.FIRST_NAME, W.LAST_NAME, W.DEPARTMENT FROM Worker W JOIN ( SELECT DEPARTMENT, MAX(SALARY) AS MAX_SALARY FROM Worker GROUP BY DEPARTMENT) M ON W.DEPARTMENT = M.DEPARTMENT AND W.SALARY = M.MAX_SALARY;</p>
<p>&nbsp;47.SELECT DISTINCT SALARY FROM Worker ORDER BY SALARY DESC LIMIT 3;</p>
<p>&nbsp;48.SELECT Salary FROM Worker ORDER BY Salary LIMIT 3;</p>
<p>49.SELECT DEPARTMENT, SUM(SALARY) AS TOTAL_SALARY FROM Worker GROUP BY DEPARTMENT;&nbsp;</p>
<p>50.SELECT FIRST_NAME, LAST_NAME FROM Worker WHERE SALARY = (SELECT MAX(SALARY)FROM Worker);</p></body></html>